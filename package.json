    "@types/date-fns": "^2.6.0",
    "@types/debug": "4.1.5",
    "@types/fs-extra": "^9.0.4",
    "@types/http-proxy": "^1.17.14",
    "@types/jest": "^29.5.2",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^20.11.0",
    "@types/query-string": "^6.3.0",
